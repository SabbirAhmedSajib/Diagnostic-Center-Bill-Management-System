@model IEnumerable<Diagnostic_Center_Bill_Management_System.ViewModel.TestWiseReportInformationViewModel>

<table class="table">
    <tr>
        <th>
            SL
        </th>
        <th>
            Test Type Name
        </th>
        <th>
            Total no Of  Test
        </th>
        <th>
            Total Amount
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TypeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalTest)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalAmount)
            </td>
        </tr>
    }

</table>
