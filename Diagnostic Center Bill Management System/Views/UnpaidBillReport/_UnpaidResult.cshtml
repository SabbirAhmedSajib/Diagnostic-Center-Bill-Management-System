@model IEnumerable<Diagnostic_Center_Bill_Management_System.ViewModel.UnpaidResultViewModel>

<table class="table">
    <tr>
        <th>
            SL
        </th>
        <th>
            Bill Number
        </th>
        <th>
            Contact No
        </th>
        <th>
            Patient Name
        </th>
        <th>
            Bill Amount
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Billno)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contactno)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PatientName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BillAmount)
        </td>
    </tr>
}

</table>
